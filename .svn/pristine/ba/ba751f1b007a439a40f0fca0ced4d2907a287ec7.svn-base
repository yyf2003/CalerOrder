/*global*/
*
{
    margin: 0 auto;
    padding: 0;
}
body
{
    color: #666666;
    font: 12px "宋体";
}
ul, li
{
    list-style: none;
}
a
{
    text-decoration: none;
}

.clear
{
    clear: both;
    height: 0;
    font-size: 0;
    line-height: 0;
    overflow: hidden;
}
img
{
    border: none;
}
/* end global */
.Rec
{
    width: 885px;
    height: 440px;
}
.Rec01
{
    width: 825px;
    margin-top: 30px;
}
.Rectop
{
    color: #000;
    width: 300px;
    font-size: 24px;
    font-weight: bold;
    text-align: center;
    padding-bottom: 10px;
    border-bottom: #6b6b6b solid 1px;
}
.RecTtable
{
    border-collapse: collapse;
    font-size: 12px;
    margin-top: 15px;
    background: #FFF;
}

.RecTtable td
{
    border: 1px solid #000;
    height: 40px;
    line-height: 40px;
}
.RecTtable_right
{
    text-align: right;
    padding-right: 5px;
}
.RecTtable_left
{
    text-align: left;
    padding-left: 5px;
}
.RecTtable tr
{
}
.RecTtable_but
{
    margin-top: 10px;
    text-align: center;
}
.RecTtable_input
{
    background: url(../image/rec_but.gif);
    width: 71px;
    height: 27px;
    border: none;
}

.divBackColor
{
    background: #FFF;
}
